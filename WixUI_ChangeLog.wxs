<?xml version="1.0" encoding="UTF-8"?>
<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
  <Fragment>
    <UI>
      <Dialog Id="ChangeLogDlg" Width="370" Height="270" Title="!(loc.InstallDirDlg_Title)">

        <Control Id="Next" Type="PushButton" X="236" Y="243" Width="56" Height="17" Default="yes" Text="!(loc.WixUINext)" />
        <Control Id="Back" Type="PushButton" X="180" Y="243" Width="56" Height="17" Text="!(loc.WixUIBack)" />
        <Control Id="Cancel" Type="PushButton" X="304" Y="243" Width="56" Height="17" Cancel="yes" Text="!(loc.WixUICancel)">
          <Publish Event="SpawnDialog" Value="CancelDlg">1</Publish>
        </Control>

        <Control Id="Title" Type="Text" X="15" Y="6" Width="200" Height="15" Transparent="yes" NoPrefix="yes" Text="!(loc.ChangeLogTitle)" />
        <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="370" Height="44" TabSkip="no" Text="WixUI_Bmp_Banner" />
        <Control Id="BannerLine" Type="Line" X="0" Y="44" Width="370" Height="0" />
        <Control Id="BottomLine" Type="Line" X="0" Y="234" Width="370" Height="0" />

        <Control Id="ChangeLogEst" Type="Text" X="20" Y="60" Width="30" Height="30" NoPrefix="yes" Text="EST:" />
        <Control Id="ChangeLogEstText" Type="Hyperlink" X="60" Y="60"  Width="220" Height="30">
            <Text><![CDATA[<a href="http://www.id.ee/changelog">http://www.id.ee/changelog</a>]]></Text> 
        </Control>

        <Control Id="ChangeLogEng" Type="Text" X="20" Y="120" Width="30" Height="30" NoPrefix="yes" Text="ENG:" />
        <Control Id="ChangeLogEngText" Type="Hyperlink" X="60" Y="120"  Width="220" Height="30">
            <Text><![CDATA[<a href="http://www.id.ee/eng/changelog">http://www.id.ee/eng/changelog</a>]]></Text> 
        </Control>

        <Control Id="ChangeLogRus" Type="Text" X="20" Y="180" Width="30" Height="30" NoPrefix="yes" Text="RUS:" />
        <Control Id="ChangeLogRusText" Type="Hyperlink" X="60" Y="180"  Width="220" Height="30">
            <Text><![CDATA[<a href="http://www.id.ee/rus/changelog">http://www.id.ee/rus/changelog</a>]]></Text> 
        </Control>

      </Dialog>
    </UI>
  </Fragment>
</Wix>
